{
  "name": "@torus-tools/cli",
  "description": "Mutli CLI for torus tools",
  "version": "0.0.1",
  "author": "Gabriel Kardonski @gkpty",
  "bin": {
    "torus": "./bin/run"
  },
  "bugs": "https://github.com/torus-tools/cli/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.0.0",
    "@torus-tools/content": "0.0.2",
    "@torus-tools/domains": "0.0.13",
    "@torus-tools/stack": "0.0.1",
    "cli-ux": "^5.4.6",
    "dotenv": "^8.2.0",
    "node-notifier": "^8.0.0",
    "open": "^7.0.4",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "torus-build": "^0.1.0",
    "torus-deploy": "^0.8.0",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/torus-tools/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "torus",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "torus-tools/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
