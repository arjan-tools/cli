{
  "name": "arjan-cli",
  "description": "Mutli CLI for arjan tools",
  "version": "0.1.0",
  "author": "Gabriel Kardonski @gkpty",
  "bin": {
    "arjan": "./bin/run"
  },
  "bugs": "https://github.com/arjan-tools/cli/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.0.0",
    "arjan-audit": "^0.5.0",
    "arjan-deploy": "^0.6.0",
    "arjan-localize": "^1.1.3",
    "arjan-optimize": "^0.6.2",
    "cli-ux": "^5.4.6",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "chai": "^4.2.0",
    "globby": "^10.0.2",
    "mocha": "^7.1.2",
    "nyc": "^14.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/arjan-tools/cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "arjan",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "arjan-tools/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
